@model List<Rehber.Models.Entities.Kişi>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Ev Telefonu</th>
        <th>Cep Telefonu</th>
        <th>İş Telefonu</th>
        <th>Email Adres</th>
        <th>Adres</th>
        <th>Sehir</th>
    </tr>
    @foreach (var kisi in Model)
     {
        <tr>
            <td>@kisi.KisiAdi</td>
            <td>@kisi.Soyad</td>
            <td>@kisi.EvTelefon</td>
            <td>@kisi.CepTelefon</td>
            <td>@kisi.IsTelefon</td>
            <td>@kisi.EmailAdres</td>
            <td>@kisi.Adres</td>
            <td>@kisi.Sehir</td>
        </tr>
     }

</table>